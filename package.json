{
  "name": "ddd-ui-kit",
  "version": "1.2.2",
  "description": "UI Kit made by DDD team",
  "main": "dist/ddd-ui-kit.mjs",
  "module": "dist/ddd-ui-kit.mjs",
  "style": "dist/ddd-ui-kit.css",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "start": "vite",
    "build": "tsc && vite build && npm run build:types",
    "build:types": "tsc -p tsconfig.build.json"
  },
  "keywords": [
    "ui-kit",
    "ddd",
    "frontend",
    "typescript"
  ],
  "dependencies": {
    "@robocotik/react": "^1.6.0",
    "ddd-clsx": "^1.0.4"
  },
  "devDependencies": {
    "@fontsource/golos-ui": "^4.5.3",
    "@robocotik/react": "^1.6.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/node": "^24.7.2",
    "globals": "^16.4.0",
    "sass": "^1.93.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.2.7",
    "vite-plugin-pwa": "^1.1.0",
    "vite-plugin-static-copy": "^3.1.4",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
